<UserControl x:Class="GSharp.Graphic.Holes.PrevConnectHole"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSharp.Graphic.Holes"
             mc:Ignorable="d" 
             Height="10" d:DesignWidth="300" SnapsToDevicePixels="True">
    <DockPanel>
        <Rectangle Fill="#D5404D" Height="2" VerticalAlignment="Bottom" Width="10" />
        <Path Data="M 0,11 L 10,1 20,11 Z" StrokeThickness="2" StrokeLineJoin="Round" Margin="-3, 0" Stroke="#D5404D" Fill="White" />
        <Rectangle Fill="#D5404D" Height="2" VerticalAlignment="Bottom" />
    </DockPanel>
</UserControl>
