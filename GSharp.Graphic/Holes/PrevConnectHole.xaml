<local:BaseHole x:Class="GSharp.Graphic.Holes.PrevConnectHole"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSharp.Graphic.Holes"
             mc:Ignorable="d" 
             Height="10" d:DesignWidth="300">
    <DockPanel>
        <Rectangle Fill="DarkRed" Height="1" VerticalAlignment="Bottom" Width="11" />
        <Grid Margin="-1,0">
            <Polygon Points="0,10 10,0 20,10" Fill="Red" />
            <Polygon Points="0,10 10,0 20,10 19,10 10,1 1,10" Fill="DarkRed" />
        </Grid>
        <Rectangle Fill="DarkRed" Height="1" VerticalAlignment="Bottom" />
    </DockPanel>
</local:BaseHole>
