<local:BaseObjectHole x:Class="GSharp.Graphic.Holes.StringHole"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSharp.Graphic.Holes"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="30">
    <Border Background="#C8C8C8" CornerRadius="10" MinHeight="30" MinWidth="26">
        <Grid>
            <TextBox x:Name="BlockString" Margin="5" TextAlignment="Center" VerticalContentAlignment="Center"></TextBox>
            <Border x:Name="BlockHole"></Border>
        </Grid>
    </Border>
</local:BaseObjectHole>
