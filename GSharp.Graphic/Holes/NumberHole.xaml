<local:BaseObjectHole x:Class="GSharp.Graphic.Holes.NumberHole"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSharp.Graphic.Holes"
             xmlns:object="clr-namespace:GSharp.Graphic.Objects"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="30">
    <Border Background="#C8C8C8" CornerRadius="2">
        <Grid>
            <Border x:Name="BlockHole"></Border>
            <object:NumberConstBlock x:Name="BlockNumber" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
        </Grid>
    </Border>
</local:BaseObjectHole>
