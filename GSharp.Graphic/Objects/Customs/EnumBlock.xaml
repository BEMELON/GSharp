<core:ObjectBlock x:Class="GSharp.Graphic.Objects.Customs.EnumBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:objects="clr-namespace:GSharp.Graphic.Objects"
             xmlns:local="clr-namespace:GSharp.Graphic.Objects.Customs"
             xmlns:core="clr-namespace:GSharp.Graphic.Blocks"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100" HorizontalAlignment="Left" VerticalAlignment="Top">

    <core:ObjectBlock.Resources>
        <ResourceDictionary Source="/GSharp.Graphic;component/ResourceDictionary.xaml"/>
    </core:ObjectBlock.Resources>

    <Border CornerRadius="2" Padding="3" Background="#FFBB42C3" HorizontalAlignment="Left" VerticalAlignment="Top" BorderBrush="#FF8B3191" BorderThickness="1">
        <ComboBox x:Name="EnumName" Height="20" DisplayMemberPath="FriendlyName" SelectionChanged="EnumName_SelectionChanged" Background="#FF79297E" Foreground="White" Style="{StaticResource ComboBoxStyle}"/>
    </Border>
</core:ObjectBlock>
