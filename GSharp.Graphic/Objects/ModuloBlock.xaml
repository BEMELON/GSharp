<local:NumberBlock x:Class="GSharp.Graphic.Objects.ModuloBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSharp.Graphic.Objects"
             xmlns:core="clr-namespace:GSharp.Graphic.Core"
             xmlns:hole="clr-namespace:GSharp.Graphic.Holes"
             mc:Ignorable="d" HorizontalAlignment="Left" VerticalAlignment="Top">

    <Border HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF5CB712" BorderBrush="#FF4C970F" BorderThickness="1" CornerRadius="2" Padding="3">
        <StackPanel Orientation="Horizontal">
            <hole:NumberHole x:Name="NumberHole1" VerticalAlignment="Center" Foreground="#FF3B760B"/>
            <TextBlock Margin="2,0,4,0" VerticalAlignment="Center" FontWeight="Bold" Foreground="White" Text="을(를)"/>
            <hole:NumberHole x:Name="NumberHole2" VerticalAlignment="Center" Foreground="#FF3B760B"/>
            <TextBlock Margin="2,0,4,0" VerticalAlignment="Center" FontWeight="Bold" Foreground="White" Text="로 나눈 나머지"/>
        </StackPanel>
    </Border>
</local:NumberBlock>
