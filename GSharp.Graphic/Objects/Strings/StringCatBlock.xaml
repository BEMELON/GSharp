<objects:StringBlock x:Class="GSharp.Graphic.Objects.Strings.StringCatBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:objects="clr-namespace:GSharp.Graphic.Objects"
             xmlns:local="clr-namespace:GSharp.Graphic.Objects.Strings"
             xmlns:hole="clr-namespace:GSharp.Graphic.Holes"
             mc:Ignorable="d" HorizontalAlignment="Left" VerticalAlignment="Top">

    <Border CornerRadius="2" Padding="3" Background="#FFBB42C3" HorizontalAlignment="Left" VerticalAlignment="Top" BorderBrush="#FF8B3191" BorderThickness="1">
        <StackPanel Orientation="Horizontal">
            <hole:StringHole x:Name="StringHole1" VerticalAlignment="Center" Foreground="#FF79297E"/>
            <TextBlock Margin="3,0" VerticalAlignment="Center" FontWeight="Bold" Foreground="White" Text="와" TextAlignment="Center"/>
            <hole:StringHole x:Name="StringHole2" VerticalAlignment="Center" Foreground="#FF79297E"/>
            <TextBlock Margin="3,0,0,0" VerticalAlignment="Center" FontWeight="Bold" Foreground="White" Text="결합" TextAlignment="Center"/>
        </StackPanel>
    </Border>
</objects:StringBlock>
