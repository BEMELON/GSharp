<objects:NumberBlock x:Class="GSharp.Graphic.Objects.Numbers.LengthBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:objects="clr-namespace:GSharp.Graphic.Objects"
             xmlns:local="clr-namespace:GSharp.Graphic.Objects.Numbers"
             xmlns:core="clr-namespace:GSharp.Graphic.Core"
             xmlns:hole="clr-namespace:GSharp.Graphic.Holes"
             mc:Ignorable="d" HorizontalAlignment="Left" VerticalAlignment="Top" SnapsToDevicePixels="True">

    <Border HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF5CB712" BorderBrush="#FF4C970F" BorderThickness="1" CornerRadius="2" Padding="3">
        <StackPanel Orientation="Horizontal">
            <hole:StringHole x:Name="StringHole1" VerticalAlignment="Center" Foreground="#FF3B760B"/>
            <TextBlock x:Name="Operator" Margin="4,0,0,0" VerticalAlignment="Center" FontWeight="Bold" Foreground="White" Text="의 길이" TextAlignment="Center"/>
        </StackPanel>
    </Border>
</objects:NumberBlock>
