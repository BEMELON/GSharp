<core:ObjectBlock x:Class="GSharp.Graphic.Objects.MinusBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSharp.Graphic.Objects"
             xmlns:core="clr-namespace:GSharp.Graphic.Core"
             xmlns:hole="clr-namespace:GSharp.Graphic.Holes"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300" HorizontalAlignment="Left" VerticalAlignment="Top">
    <Border CornerRadius="10" Padding="6" Background="White" HorizontalAlignment="Left" VerticalAlignment="Top" BorderBrush="#D5404D" BorderThickness="2">
        <StackPanel Orientation="Horizontal">
            <hole:ObjectHole x:Name="ObjectHole1" />
            <TextBlock VerticalAlignment="Center" Margin="5,0">-</TextBlock>
            <hole:ObjectHole x:Name="ObjectHole2" />
        </StackPanel>
    </Border>
</core:ObjectBlock>
