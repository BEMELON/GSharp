<core:ObjectBlock x:Class="GSharp.Graphic.Objects.MemberBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSharp.Graphic.Objects"
             xmlns:core="clr-namespace:GSharp.Graphic.Core"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100" HorizontalAlignment="Left" VerticalAlignment="Top">
    <Border CornerRadius="10" Padding="10" Background="Green" HorizontalAlignment="Left" VerticalAlignment="Top">
        <ComboBox x:Name="MemberCombo" HorizontalAlignment="Center" VerticalAlignment="Center" SelectedIndex="0">
            <ComboBoxItem>조태상</ComboBoxItem>
            <ComboBoxItem>위가혜</ComboBoxItem>
            <ComboBoxItem>소현섭</ComboBoxItem>
        </ComboBox>
    </Border>
</core:ObjectBlock>
