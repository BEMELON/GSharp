<core:ObjectBlock x:Class="GSharp.Graphic.Objects.PropertyBlock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSharp.Graphic.Objects"
             xmlns:core="clr-namespace:GSharp.Graphic.Core"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100" HorizontalAlignment="Left" VerticalAlignment="Top">
    
    <Border CornerRadius="10" Padding="10" Background="Green" HorizontalAlignment="Left" VerticalAlignment="Top">
        <TextBlock x:Name="PropertyName" Foreground="White"></TextBlock>
    </Border>
</core:ObjectBlock>
